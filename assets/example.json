{
  "filename": "example.pdf",
  "documentTitle": "Example PDF",
  "pageSize": "letter",
  "pageMargins": {
    "top": 20,
    "right": 20,
    "bottom": 20,
    "left": 20
  },
  "fonts": [
    {
      "family_name": "Inter",
      "fonts": [
        {
          "source": "./assets/fonts/inter-static/Inter-Bold.ttf",
          "weight": "Bold",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Thin-BETA.ttf",
          "weight": "Thin",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-SemiBold.ttf",
          "weight": "SemiBold",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-ThinItalic-BETA.ttf",
          "weight": "Thin",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-SemiBoldItalic.ttf",
          "weight": "SemiBold",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-ExtraLight-BETA.ttf",
          "weight": "ExtraLight",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-ExtraBoldItalic.ttf",
          "weight": "ExtraBold",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Light-BETA.ttf",
          "weight": "Light",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-UI-BlackItalic.ttf",
          "weight": "Black",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-BoldItalic.ttf",
          "weight": "Bold",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Regular.ttf",
          "weight": "Regular",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Italic.ttf",
          "weight": "Regular",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-ExtraLightItalic-BETA.ttf",
          "weight": "ExtraLight",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-LightItalic-BETA.ttf",
          "weight": "Light",
          "style": "Italic"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-ExtraBold.ttf",
          "weight": "ExtraBold",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Medium.ttf",
          "weight": "Medium",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-Black.ttf",
          "weight": "Black",
          "style": "Normal"
        },
        {
          "source": "./assets/fonts/inter-static/Inter-MediumItalic.ttf",
          "weight": "Medium",
          "style": "Italic"
        }
      ]
    }
  ],
  "stylesheet": {
    "root": {
      "debug": false,
      "color": "#444A5A",
      "font": {
        "size": "16pt",
        "weight": "Regular",
        "family": "Inter"
      }
    },
    "flagpole": {
      "backgroundColor": "#EDF6F9",
      "padding": {
        "top": "24pt",
        "right": "24pt",
        "left": "24pt",
        "bottom": "24pt"
      },
      "border": {
        "width": {
          "left": "2pt"
        },
        "color": "#0080AD",
        "radius": {
          "topRight": "12pt",
          "bottomRight": "12pt"
        }
      }
    },
    "h3": {
      "font": {
        "size": "12pt",
        "weight": "ExtraBold"
      },
      "margin": {
        "bottom": "16pt"
      }
    },
    "p": {}
  },
  "root": {
    "type": "Styled",
    "styles": ["root"],
    "children": [
      {
        "type": "Styled",
        "styles": ["flagpole"],
        "children": [
          {
            "type": "Text",
            "styles": ["h3"],
            "children": ["HOW OTHERS MIGHT VIEW YOU"]
          },
          {
            "type": "Text",
            "styles": ["p"],
            "children": [
              "Others are likely to view you as someone able to make decisions when required. While you might spend more time than others evaluating your options before committing, you are likely able to arrive at a decision in most instances."
            ]
          }
        ]
      }
    ]
  }
}
